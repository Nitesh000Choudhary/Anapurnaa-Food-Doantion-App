<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="200dp"
    android:height="200dp"
    android:viewportWidth="200"
    android:viewportHeight="200">

    <!-- Outer Circle -->
    <path
        android:pathData="M100,10 A90,90 0 1,1 99.9,10 Z"
        android:strokeWidth="10"
        android:strokeColor="#28D4FF"
        android:fillColor="@color/transparent"/>

    <!-- Inner Circle -->
    <path
        android:pathData="M100,25 A75,75 0 1,1 99.9,25 Z"
        android:fillColor="#FFFFFF"
        android:strokeWidth="5"
        android:strokeColor="#86E6FF"/>

    <!-- Refresh Symbol (Circular Arrow) -->
    <path
        android:pathData="M100,50 A50,50 0 1,1 50,100"
        android:strokeWidth="10"
        android:strokeColor="#000000"
        android:fillColor="@color/transparent"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"/>

    <!-- Arrowhead -->
    <path
        android:pathData="M50,100 L65,110 M50,100 L60,85"
        android:strokeWidth="10"
        android:strokeColor="#000303"
        android:strokeLineCap="round"
        android:strokeLineJoin="round"/>

</vector>
